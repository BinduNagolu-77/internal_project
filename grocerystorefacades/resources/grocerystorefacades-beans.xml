<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="beans.xsd">

	<bean class="de.hybris.platform.commercefacades.product.data.ProductData">
		<property name="genders" type="java.util.List&lt;com.hybris.grocerystore.facades.product.data.GenderData>"/>
	</bean>

	<bean class="com.hybris.grocerystore.facades.product.data.GenderData">
		<property name="code" type="String"/>
		<property name="name" type="String"/>
	</bean>
	
	<bean class="de.hybris.platform.commercefacades.product.data.ProductData">
		<property name="countryOrigin" type="String"/>
		<property name="weight" type="Double"/>
		<property name="weightUnit" type="String"/>
		<property name="shelfLife" type="String"/>
		<property name="brandName" type="String"/>
	</bean>
	
	<enum class="de.hybris.platform.commercefacades.product.ProductOption">
	 	<value>WEIGHT</value>
	 	<value>WEIGHTUNIT</value>
	</enum>
	
	
</beans>
